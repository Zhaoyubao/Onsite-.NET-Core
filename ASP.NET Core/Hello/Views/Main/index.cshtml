<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>ASP.NET Core</title>
    <link rel="stylesheet" href="css/style.css">
</head>
<body>
    <h1>Hello, @ViewBag.Name!</h1>
    <h2>Name: @ViewBag.Name</h2>
    <form action="/submit" method="post">
        <input type="text" name="name">
        <button type="submit">Submit</button>
    </form>
    <ul>
    @foreach(var cat in @ViewBag.Cats)
    {
        <li>Name: @cat.Name Age: @cat.Age</li>
    }
    </ul>
    @{
        string display = "CodingDojo";
    }
    <p>@display</p> 
</body>
</html>